        "resume_state": "/home/jovyan/cloud/Palette/results/25-05-18-ssim-mae/checkpoints/23500"
    },
    "datasets": {
        "train": {
            "which_dataset": {
                "name": [
                    "data.dataset",
                    "InpaintDataset"
                ],
                "args": {
                    "data_root": "/home/jovyan/cloud/Palette/datasets/ipfz/_flist/ipfz-real-supervised/train.flist",
                    "data_len": -1,
                    "mask_config": {
                        "mask_mode": "from_image"
