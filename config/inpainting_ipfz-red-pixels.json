{
    "name": "test_inpainting_ipfz",
    "gpu_ids": [
        0
    ],
    "seed": -1,
    "finetune_norm": false,
    "path": {
        "base_dir": "experiments",
        "code": "code",
        "tb_logger": "tb_logger",
        "results": "results",
